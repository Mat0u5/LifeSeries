{
  "schemaVersion": 1,
  "id": "lifeseries",
  "version": "v-${modVer}+${baseModVer}+${minecraftVer}",
  "name": "Life Series Trivia Takeover",
  "description": "A mod of Mat0u5's Life Series mod changing stuff like the model of the Trivia bots or changing snails to be VHsnails",
  "authors": [
    "Mat0u5",
    "TheCatKing01"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/TheCatKing01/LifeSeries-Trivia-Takeover"
  },
  "license": "MIT",
  "icon": "assets/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mat0u5.lifeseries.Main"
    ],
    "client": [
      "net.mat0u5.lifeseries.MainClient"
    ],
    "voicechat": [
      "net.mat0u5.lifeseries.voicechat.VoicechatMain"
    ]
  },
  "mixins": [
    "lifeseries.mixins.json",
    {
      "config": "lifeseries.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "minecraft": "${minecraftVer}",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "bil": ">=1.1.10",
    "polymer-bundled": ">=0.10.0"
  },
  "breaks": {
    "pastlife": "*"
  }
}